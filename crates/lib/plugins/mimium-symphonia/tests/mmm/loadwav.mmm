//Sampler_mono! macro returns a closure that takes playback position in samples as an argument
let sampler = Sampler_mono!("tests/assets/count_100_by_0_01_f32_48000Hz.wav")
fn counter(){
    self+1.0 
}
//add -1.0 offset so that the counter start from 0 at t=0
fn dsp(){
    counter()-1.0 |> sampler
}