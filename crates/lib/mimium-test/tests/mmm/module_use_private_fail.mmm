// Test that use statement respects visibility
// Attempting to use a private function should fail

mod mymath {
    fn private_add(x, y) {
        x + y
    }
    pub fn public_mul(x, y) {
        x * y
    }
}

// This should fail: private_add is not public
use mymath::private_add

fn dsp() {
    private_add(10.0, 5.0)
}
