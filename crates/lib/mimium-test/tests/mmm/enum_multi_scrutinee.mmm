// Multi-scrutinee pattern matching test
// Match on tuple of enums simultaneously

fn test_simple(a: float, b: float){
  match (a, b) {
    (1, 1) => 10,
    (1, 2) => 20,
    (2, 1) => 30,
    _ => 100
  }
}

let x = test_simple(1, 1)
let y = test_simple(1, 2)
let z = test_simple(2, 1)
let a = test_simple(3, 3)

fn dsp(){
    // x = 10, y = 20, z = 30, a = 100
    // Total = 160
    x+y+z+a
}
