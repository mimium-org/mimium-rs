  let arr = [10.0, 20.0, 40.0]
  
  // Access the first element
  let first = arr[0] 
  
  // Access with interpolation (should give 15.0)
  let interp = arr[0.5]
  
  // Modify the third element /todo: array assignment
  // arr[2] = 40.0
  
