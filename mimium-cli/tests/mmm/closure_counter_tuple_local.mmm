// this creates two counters on non-global scope with tuple assignment
fn makecounter(){
    let x = 0.0
    let countup = | |{
        let res = x
        x = x+1.0
        res
    }

    let y = 0.0
    let countdown = | |{
        let res = y
        y = y-1.0
        res
    }
    (countup,0.0,countdown)
}
fn dsp(){
    let (c1,_,c2) = makecounter();
    (c1(),c2())
}