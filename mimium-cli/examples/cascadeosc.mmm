include("osc.mmm")
fn osc(freq){
  sinwave(freq,0.0)
}

fn cascade(n,gen:()->(float)->float){
    let g = gen()
    if (n>0.0){
        let c = cascade(n - 1.0 ,gen)
        |rate| rate + g(rate/2.0)* 0.5* rate*(1.0-(1.0/(n*2.0))) |> c 
    }else{
        |rate| g(rate) 
    }
}
let n =20.0
let myosc = cascade(n, | | osc);

fn dsp(){
    let r = myosc(200.0)
    (r,r)
}