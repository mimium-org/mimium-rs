include("osc.mmm")

fn osc(freq){
  sinwave(freq,0.0)
}
fn amosc(freq,rate){
  osc(freq + osc(rate)*4000.0)
}
fn replicate(n,gen:()->(float,float)->float){
    if (n>0.0){
        let c = replicate(n - 1.0,gen)
        let g = gen()
        |x,rate| g(x,rate) + c(x+100.0,rate+0.1)
    }else{
        |x,rate|  0
    }
}
let n = 20.0
let multiosc = replicate(n,| | amosc);
fn dsp(){
    let res = multiosc(4000.0,0.5) / n
    res
}