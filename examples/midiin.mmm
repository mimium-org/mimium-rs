let _ = set_midi_port("from Max 1")
let pi = 3.14159265359
let sr = samplerate
fn phasor(freq){
  (self + freq/sr)%1.0
}
fn osc(freq){
  sin(phasor(freq)*pi*2.0)
}
fn midi_to_hz(note){
    440.0* (2.0 ^((note-69.0)/12.0))
}
fn dsp(){
    let note = midi_note_mono!(0.0,69.0,127.0);
    let sig = note.pitch |> midi_to_hz |> osc 
    let gain = (note.velocity /127.0) |> Probe!("gain")
    let r = sig * gain |> Probe!("out")
    (r,r)
}